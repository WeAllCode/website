{
  "name": "coderdojochi",
  "scripts": {},
  "env": {
    "CONTACT_EMAIL": {
      "required": true
    },
    "DEBUG": {
      "required": true,
      "value": "True"
    },
    "DISABLE_COLLECTSTATIC": {
      "required": true,
      "value": "True"
    },
    "DEFAULT_FROM_EMAIL": {
      "required": true
    },
    "DJANGO_SECURE_SSL_REDIRECT": {
      "required": true
    },
    "ENABLE_DEV_FIXTURES": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "SENDGRID_API_KEY": {
      "required": true
    },
    "SENDGRID_UNSUB_CLASSANNOUNCE": {
      "required": true
    },
    "SLACK_WEBHOOK_URL": {
      "required": true
    },
    "RECAPTCHA_PUBLIC_KEY": {
      "required": true
    },
    "RECAPTCHA_PRIVATE_KEY": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql",
    "logdna"
  ],
  "buildpacks": [{
    "url": "heroku/python"
  }]
}
